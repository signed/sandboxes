{
  "name": "esm-ts-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@7.1.7",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs.js"
  },
  "scripts": {
    "ava": "TS_NODE_PROJECT='./tsconfig.ava.json' ava",
    "mocha": "TS_NODE_PROJECT='./tsconfig.mocha.json' mocha",
    "vitest": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@signed/prettier-config": "0.1.0",
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "ava": "4.3.0",
    "chai": "4.3.6",
    "mocha": "10.0.0",
    "prettier": "2.6.2",
    "ts-node": "10.8.1",
    "typescript": "4.7.3",
    "vitest": "0.14.1"
  },
  "sideEffects": false,
  "prettier": "@signed/prettier-config",
  "ava": {
    "files": [
      "**/*.ava.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm/transpile-only.mjs"
    ]
  }
}
