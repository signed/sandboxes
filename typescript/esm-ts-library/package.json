{
  "name": "esm-cjs-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@7.1.7",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs.js"
  },
  "scripts": {
    "ava": "TS_NODE_PROJECT='./tsconfig.ava.json' ava",
    "typecheck": "tsc --noEmit",
    "coverage": "jest --coverage --collectCoverageFrom=src/*.ts"
  },
  "devDependencies": {
    "@signed/prettier-config": "0.1.0",
    "ava": "4.3.0",
    "prettier": "2.6.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "sideEffects": false,
  "prettier": "@signed/prettier-config",
  "ava": {
    "files": [
      "**/*.ava.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
