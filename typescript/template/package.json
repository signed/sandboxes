{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom=src/*.ts"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "testRegex": ".*\\.tests\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "displayName": "catch me",
    "notify": false,
    "notifyMode": "failure-success",
    "resetMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/tests/matchers/toEndWith.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "jest": "24.1.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.3.3333"
  }
}
