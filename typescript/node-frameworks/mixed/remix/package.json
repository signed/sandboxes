{
  "name": "@sandbox/remix",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"node --watch-path server.js --watch server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "nit-pick": "pnpm test && pnpm typecheck",
    "clean": "rimraf .cache build public/build"
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.2.0",
    "@remix-run/express": "2.2.0",
    "@remix-run/node": "2.2.0",
    "@remix-run/react": "2.2.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "isbot": "3.7.0",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "2.2.0",
    "@tsconfig/remix": "2.0.1",
    "@types/compression": "1.7.4",
    "@types/express": "4.17.20",
    "@types/morgan": "1.9.7",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/source-map-support": "0.5.9",
    "chokidar": "3.5.3",
    "rimraf": "5.0.5"
  }
}
