{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rulesDirectory": [],
  "rules": {
    "interface-name": false,
    "no-console": false,
    "max-classes-per-file": false,
    "import-name": [true, {
      "application": "Application",
      "list": "List"
    }]
  },
  "jsRules": {
    "eofline": false
  }
}